ue);\r\n    };\r\n    const close = () => {\r\n        setDisplay(false)\r\n    };\r\n\r\n\r\n    if (display) {\r\n        return (\r\n            ReactDOM.createPortal(\r\n                <div className={\"modal-wrapper\"}>\r\n                    <div onClick={close} className={\"modal-backdrop\"} />\r\n                    <div className={\"modal-box\"}>\r\n                        {props.children}\r\n                    </div>\r\n                </div>, document.getElementById(\"modal-root\")\r\n            )\r\n        )\r\n    };\r\n    return null;\r\n}\r\n)\r\n\r\nexport default Modal","C:\\Users\\diego\\OneDrive\\Escritorio\\Let's_Coder\\project3\\my-app\\src\\components\\register.js",["167","168","169"],"import React from 'react';\r\nimport {BrowserRouter as Router, Switch, Route, Link} from 'react-router-dom'\r\n\r\nfunction Register() {\r\n    return (\r\n        <div className=\"register app grid\">\r\n            <div className=\"registerLogo\"><img src=\"../../img/LabTool_logo.png\" alt=\"LabTool_logo\"/></div>\r\n            <Link to=\"/signup\"className=\"button1 signupButtonRegister\"><p>SIGN UP</p></Link>\r\n            <Link to=\"/signin\" className=\"button1 signinButtonRegister\"><p>SIGN IN</p></Link>\r\n        </div>\r\n\r\n    )\r\n}\r\n\r\nexport default Register","C:\\Users\\diego\\OneDrive\\Escritorio\\Let's_Coder\\project3\\my-app\\src\\components\\productSheet.js",["170","171","172","173","174","175"],"import React from 'react';\r\nimport axios from 'axios';\r\nimport {useState, useEffect} from 'react'\r\nimport { Link , Route, withRouter} from 'react-router-dom'\r\nimport Modal from \"./modal\"\r\nimport ModalResponse from './modalResponse';\r\nimport ModalConfirm from './modalConfirm';\r\nimport SuccessResponse from \"./successResponse\";\r\nimport ErrorResponse from \"./errorResponse\"\r\nimport apiURL from '../services/apiURL'\r\n\r\nimport {Redirect} from 'react-router-dom'\r\n\r\n\r\n\r\nfunction ProductSheet(props) {\r\n\r\n    // 